{
  "env": {
    "es6": true,
    "node": true
  },
  "extends": "airbnb-typescript/base",
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "plugins": ["@typescript-eslint"],
  "rules": {
    "no-console": 1
  },
  "parserOptions": {
    "project": "./tsconfig.json",
    "ecmaVersion": 2018,
    "sourceType": "module"
  }
}
